<UserControl x:Class="WhatsMyStatus_DnD_.Views.Dialogs.HealthChangePopup"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="200" d:DesignWidth="300">
    
    <StackPanel Width="300" x:Name="LayoutRoot" Background="{StaticResource PhoneChromeBrush}">
        <TextBlock Name="description" TextWrapping="Wrap"/>
        <TextBox Name="value" InputScope="Number"/>
        <Button Name="okButton" Content="Ok" />
    </StackPanel>
</UserControl>